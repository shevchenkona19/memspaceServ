{
  "name": "serverapp",
  "version": "1.0.0",
  "main": "src/server/app.js",
  "scripts": {
    "start": "docker-compose up db web",
    "dc:install": "docker-compose run npm install && npm run dc:migrate",
    "migration:create": "./node_modules/.bin/sequelize migration:create --name",
    "seed:create": "./node_modules/.bin/sequelize seed:generate --name",
    "seed:up": "docker-compose run web ./node_modules/.bin/sequelize db:seed:all --config ./development.json",
    "dc:seed": "docker-compose up -d db && docker-compose run web node_modules/.bin/wait-port db:5432 && npm run seed:up && docker-compose stop db",
    "dc:migrate": "docker-compose up -d db && docker-compose run web node_modules/.bin/wait-port db:5432 && npm run migration:up && docker-compose stop db",
    "migration:up": "docker-compose run web node_modules/.bin/sequelize db:migrate --config ./development.json",
    "dc:uninstall": "docker-compose down -v",
    "webpack:build": "env-cmd .env webpack --mode production --config webpack.config.js",
    "dc:start": "nodemon src/app.js",
    "dc:rebuild": "docker-compose run npm install",
    "admin:start": "env-cmd .env webpack --mode development --config webpack.development.config.js",
    "dc:startAdmin": "docker-compose run web npm run admin:start",
    "dc:buildAdmin": "docker-compose run web npm run webpack:build"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@material-ui/core": "^3.8.3",
    "@material-ui/icons": "^3.0.2",
    "async-request": "^1.2.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "env-cmd": "^8.0.2",
    "express": "^4.16.2",
    "firebase-admin": "^6.2.0",
    "helmet": "^3.12.1",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "nodemon": "^1.17.5",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "pg": "^7.4.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "request": "^2.83.0",
    "sequelize": "^5.15.1",
    "sequelize-cli": "^4.0.0",
    "sharp": "^0.20.3",
    "wait-port": "^0.2.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0"
  }
}
